<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <nav>
      <ul>
        <li><a href="/">Home Page</a></li>
        <li><a href="/new">Send New Message</a></li>
      </ul>
    </nav>
    
    <% messages.forEach(function(message){ %>
      <ul>
        <li>
          <%= message.text %>
        </li>
        <li>
          <%= message.user %>
        </li>
        <li>
          <%= message.added %>
        </li>
      </ul>
    <% }); %>
    
</body>
</html>
